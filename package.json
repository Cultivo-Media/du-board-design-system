  "license": "MIT",
  "scripts": {
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "yarn lint --fix",
    "lint:report": "yarn lint -f node_modules/eslint-detailed-reporter/lib/detailed.js -o coverage/eslint.html",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:deploy": "build-storybook && gh-pages -d storybook-static",
    "build": "rm -rf dist && rollup -c",
    "postinstall": "postinstall-build dist"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "postinstall-build": "^5.0.2",
    "styled-components": "3.2.6",
    "styled-icons": "2.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.0.0-beta.53",
    "@babel/preset-react": "7.0.0-beta.53",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-plugin-external-helpers": "6.22.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "rollup": "0.62.0",
    "rollup-plugin-babel": "3.0.7",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-jsx": "1.0.3",
    "rollup-plugin-node-resolve": "3.3.0"
  },
  "peerDependencies": {
    "styled-components": ">=2.0 || >= 3.0"
  }
